a = "hello"
A = "HELLO"
b = "world"

c = a + ' ' + b
print(c)

d = a * 3
print(d)

e = len(b)
print(e)

print(a.count('o')) #출력값 = 2
print(a.find('l')) #출력값 = 2, 없는 값을 찾을 경우 -1
print(a.index('l')) #출력값 = 2, 없는 값을 찾을 경우 에러
print(','.join(a)) #출력값 = h,e,l,l,o
print(a.replace('h','d')) #출력값 = dello
print(a.split('e')) #출력값 = ['h', 'llo']
print(a.upper()) #출력값 = HELLO
print(A.lower()) #출력값 = hello
print(a) #함수를 실행해도 a의 원본 데이터는 변하지 않는다.

a = 128
print(type(a)) #int(정수_양의 정수, 0, 음의 정수)
num_str ="720"
print(type(num_str)) #str(문자열)
num = int(num_str) #자료형 변환
print(type(num))

n = 100
n_str = str(n)
print(type(n_str))

