#35.
name1 = "김민수"
age1 = 10
name2 = "이철희"
age2 = 13
print('이름:',name1,'나이:',age1)
print('이름: %s 나이: %d' %(name1,age1))
# %s = 문자열, %d = 정수값, %f = 실수값, %c = 문자값
#36.
print('이름: {0} 나이: {1}'.format(name1,age1))
#37.
print(f'이름: {name1} 나이: {age1}')
#38.
